  <div class="page-header">
    <h1>配給履歴</h1>
  </div>

<h2><%= @company.name %>の「<%=@device.uuid%>」の配給履歴</h2>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>開始日時</th>
      <th>終了日時</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @supply_historys.each do |supply_history| %>
      <tr>
        <td><%= supply_history.id %></td>
        <td><%= supply_history.start_date %></td>
        <td><%= supply_history.end_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
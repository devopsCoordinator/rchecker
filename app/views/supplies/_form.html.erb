<%= simple_form_for(@supply) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :uuid %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :elapsed_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
